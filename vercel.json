{
    "version": 2,
    "env": {
        "DEVTO_API_KEY": "@shop_api_devto_api_key"
    },
    "builds": [{
        "src": "src/main.ts",
        "use": "@vercel/node"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ]
    }]
}
